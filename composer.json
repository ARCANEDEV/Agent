{
    "name":        "arcanedev/agent",
    "description": "A PHP user agent parser based on Mobiledetect.",
    "keywords":    ["arcanedev", "laravel", "mobile", "detect", "detector", "devices", "user agent"],
    "homepage":    "https://github.com/ARCANEDEV/Agent",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                          "^7.1.3",
        "arcanedev/support":            "^4.5",
        "mobiledetect/mobiledetectlib": "^2.8",
        "jaybizzle/crawler-detect":     "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit":     "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Agent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Agent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\Agent\\AgentServiceProvider"
            ]
        }
    }
}
